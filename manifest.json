{
  "manifest_version": 3,
  "name": "YouTube Sentiment Analyzer",
  "version": "1.0",
  "description": "Analyze sentiments of YouTube video comments.",
  "permissions": ["tabs"],
  "host_permissions": ["https://www.youtube.com/*"],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "logo.png",
          "48": "logo.png",
          "128": "logo.png"
      }
  }
}
